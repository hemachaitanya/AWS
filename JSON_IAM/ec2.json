{
    "Version" : "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Actions": [
                "ec2:start",
                "ec2:stop"   
            ],
            "Resource": [
                "arn:aws:ec2:us-east-1::",
                "arn:aws:ec2:us-east-1::"
            ]
        }
    ]
}


{
    "Version" : "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Actions": "*",
            "Resource":"ec2:*" 
        },
        {
            "Effect": "Deny",
            "Actions": "ec2:TerminateInstances",
            "Resource":"ec2:*"    
        }
        
    ]
}
